{
  "name": "@replayweb/extension",
  "private": true,
  "version": "1.0.0",
  "description": "Chrome extension for ReplayWeb",
  "main": "./dist/app/main.js",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --config webpack.config.js",
    "build:dev": "cross-env NODE_ENV=development webpack --watch --progress --hide-modules --config webpack.config.js",
    "clean": "rm -r dist/",
    "version": "node extension/manifestUpdater.js && git add extension/manifest.json",
    "package": "crx pack dist/ext --crx-version 3 -o dist/replayui.crx",
    "test": "BABEL_ENV=test jest --coverage",
    "test:watch": "BABEL_ENV=test jest --watch"
  },
  "author": "",
  "license": "AGPL",
  "dependencies": {
    "@replayweb/utils": "link:../utils",
    "antd": "~3.8.0",
    "deep-equal": "^1.0.1",
    "dexie": "^1.5.1",
    "event-emitter": "^0.3.5",
    "github-api": "^3.2.2",
    "lodash.isequal": "^4.5.0",
    "react": "^16.9.0",
    "react-dnd": "^2.6.0",
    "react-dnd-html5-backend": "^2.6.0",
    "react-dom": "^16.9.0",
    "react-redux": "^5.0.5",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "autoprefixer": "^7.2.5",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^24.0.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-import": "^1.11.0",
    "babel-plugin-rewire": "^1.2.0",
    "clean-webpack-plugin": "^0.1.19",
    "codemirror": "^5.33.0",
    "copy-webpack-plugin": "^4.5.2",
    "cross-env": "^5.1.3",
    "crx": "^5.0.0",
    "css-loader": "^0.28.8",
    "eslint": "^4.15.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-html": "^3.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jest": "^22.15.2",
    "eslint-plugin-node": "^5.1.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^7.1.0",
    "eslint-plugin-standard": "^3.0.1",
    "hard-source-webpack-plugin": "~0.10.1",
    "html-webpack-plugin": "^2.29.0",
    "keysim": "^2.0.0",
    "mocha": "~5.0.5",
    "moment": "^2.20.1",
    "node-sass": "^4.5.3",
    "postcss-loader": "^2.0.10",
    "prop-types": "^15.7.2",
    "raw-loader": "^0.5.1",
    "react-codemirror2": "~4.0.0",
    "react-hot-loader": "next",
    "react-test-renderer": "~16.3.1",
    "redux-logger": "^3.0.6",
    "redux-mock-store": "~1.5.3",
    "redux-thunk": "^2.2.0",
    "sass-loader": "^6.0.6",
    "string-loader": "0.0.1",
    "style-loader": "^0.18.2",
    "styled-components": "^2.4.0",
    "svg-react-loader": "^0.4.4",
    "webpack": "~4.15.1",
    "webpack-cli": "~3.0.8",
    "webpack-dev-server": "^3.8.2",
    "webpack-permissions-plugin": "0.0.2"
  }
}
